{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "cSpell.words": [
    "destructure",
    "heaslip",
    "peerdeps"
  ],
  "search.exclude": {
    ".yarn/releases/**/*": true,
    ".yarnrc": true,
    "CHANGELOG.markdown": true,
    "LICENSE": true,
    "yarn.lock": true
  }
}
